{
  "name": "coworking.party",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently pnpm:dev:*",
    "dev:next": "sst dev next dev",
    "dev:partykit": "partykit dev --live",
    "db": "concurrently pnpm:dev:db:*",
    "dev:db:turso": "turso dev --db-file ./db/dev.db",
    "dev:db:studio": "pnpm run db:studio",
    "pbs": "pnpm run build && pnpm run start",
    "build": "next build",
    "build:local": "dotenv -e .env -- next build",
    "start": "next start",
    "start:local": "concurrently pnpm:start pnpm:db:*",
    "lint": "next lint",
    "deploy": "pnpm run deploy:preview",
    "deploy:preview": "pnpm run deploy:preview:partykit && pnpm run deploy:preview:site",
    "deploy:preview:partykit": "dotenv -e .env.preview -- partykit deploy --preview preview",
    "deploy:preview:site": "dotenv -e .env.preview -- sst deploy --stage preview",
    "deploy:prod": "pnpm run deploy:prod:partykit && pnpm run deploy:prod:site",
    "deploy:prod:partykit": "dotenv -e .env.production -- partykit deploy",
    "deploy:prod:site": "dotenv -e .env.production -- sst deploy --stage production",
    "db:push": "drizzle-kit push:sqlite",
    "db:push:prod": "dotenv -e .env.production -- drizzle-kit push:sqlite",
    "db:push:preview": "dotenv -e .env.preview -- drizzle-kit push:sqlite",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.0.0",
    "@fontsource/space-grotesk": "^5.0.18",
    "@hookform/resolvers": "^3.3.4",
    "@libsql/client": "^0.6.0",
    "@next/third-parties": "^14.2.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@sentry/nextjs": "^7.112.1",
    "@tabler/icons-react": "^3.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.9",
    "humanize-duration": "^3.32.0",
    "jotai": "^2.8.0",
    "lodash": "^4.17.21",
    "next": "14.2.2",
    "next-auth": "5.0.0-beta.16",
    "partysocket": "1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "sonner": "^1.4.41",
    "sst": "ion",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@types/humanize-duration": "^3.27.4",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.17",
    "eslint": "^8",
    "eslint-config-next": "14.2.2",
    "partykit": "0.0.104",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.0.4+sha256.caa915eaae9d9aefccf50ee8aeda25a2f8684d8f9d5c6e367eaf176d97c1f89e",
  "cspell": {
    "ignorePaths": [
      "pnpm-lock.yaml"
    ]
  }
}
